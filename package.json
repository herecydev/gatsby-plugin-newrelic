  "homepage": "https://opensource.newrelic.com/projects/newrelic/gatsby-plugin-newrelic",
  "peerDependencies": {
    "gatsby": "^3.0.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/newrelic/gatsby-plugin-newrelic"
  },
  "license": "Apache-2",
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "@newrelic/winston-enricher": "^1.0.0",
    "fast-glob": "^3.2.5",
